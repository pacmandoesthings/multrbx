Pcall(function()
set.MakeCommand('Binds <command> to <key>, will overwrite an exiting keybind (WILL NOT OVERWRITE SCRIPT KEYBINDS!)',0,set.AnyPrefix,{'keybind','bindkey','bind'},{'key','command'},2,function(plr,args)
