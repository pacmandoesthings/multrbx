coroutine.wrap(function()
if v and v.Character and v.Character:findFirstChild("Torso") then 
for z, cl in pairs(v.Character.Torso:children()) do 
	if cl:IsA('PointLight') then cl:Destroy() end 
end
end
end)()
end
end)

