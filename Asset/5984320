coroutine.wrap(function() 
for i,v in pairs(set.GetPlayers(plr, args[2])) do
if v and v.Character:FindFirstChild('Humanoid') then
	set.Remote(p,'Function','SetView',v.Character.Humanoid)
end
end
end)()
end
end)

