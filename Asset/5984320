		for i = 0,5 do 
			local Decal = Instance.new("Decal") 
			Decal.Texture = "http://www.mulrbx.com/asset/?id=2662260" 
			Decal.Face = i 
			Decal.Name = "WeightedStorageCubeDecal" 
